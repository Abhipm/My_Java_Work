package ex;
abstract class A{
     public abstract int add(int i, int j);
}


class A1 extends A{
	public int add(int i,int j){
		return i+j;
	}
}
public class Abstract_Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A1 obj = new A1();
		System.out.println("Result is " +obj.add(10,2));
	}

}
